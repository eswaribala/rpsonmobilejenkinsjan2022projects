pipeline {
    agent any
    tools {
        maven 'maven'
        jdk 'java8'
    }
    stages {
        stage('Build') {
            
            steps {
                echo 'maven clean'
                bat ' mvn -f ./cucumberdemo clean install'  
            }
        }
    }
    post {
            success {
                        echo 'Now Archiving'
                        junit allowEmptyResults: true, testResults: '**/surefire-reports/*.xml'
                }
        }
}